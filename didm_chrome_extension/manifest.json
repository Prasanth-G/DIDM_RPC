{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["activeTab", "declarativeContent" ,"storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon" : {
            "16" : "images/logo.png",
            "32" : "images/logo.png",
            "64" : "images/logo.png",
            "128" : "images/logo.png"
        }
    },
    "options_page" : "options.html",
    "icons" : {
        "16" : "images/logo.png",
        "32" : "images/logo.png",
        "64" : "images/logo.png",
        "128" : "images/logo.png"
    },
    "manifest_version": 2
}